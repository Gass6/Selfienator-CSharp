<Window x:Class="Arduino.Selfienator.Views.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadWindow" Width="250" Height="150">
    <StackPanel VerticalAlignment="Center">
        <ComboBox Margin="5" ItemsSource="{Binding listOfPorts}" SelectedItem="{Binding selectedPort}" />
        <Button Margin="5" Content="Pripojiť" Command="{Binding connectComm}" CommandParameter="{Binding selectedPort}" />
    </StackPanel>
</Window>
